      "types": "./dist/errors.d.ts"
    },
    "./lib": {
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    },
    "./primitives": {
      "import": "./dist/primitives.mjs",
      "require": "./dist/primitives.cjs",
      "types": "./dist/primitives.d.ts"
    },
    "./utilities": {
      "import": "./dist/utilities.mjs",
      "require": "./dist/utilities.cjs",
      "types": "./dist/utilities.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --watch",
    "build:watch": "nodemon --exec \"vite build\" --ignore dist --watch ./src -e ts,tsx",
    "check:lint": "eslint src --max-warnings 0",
    "check:types": "tsc --noEmit --skipLibCheck",
    "dev": "vite"
