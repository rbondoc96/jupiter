  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.cjs",
      "types": "./dist/constants.d.ts"
    },
    "./enums": {
      "import": "./dist/enums.mjs",
      "require": "./dist/enums.cjs",
